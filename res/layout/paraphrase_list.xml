<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<ProgressBar android:layout_width="fill_parent"
		android:layout_height="wrap_content" style="@android:style/Widget.ProgressBar.Horizontal"
		android:id="@+id/progressBar1"></ProgressBar>
	<LinearLayout android:id="@+id/linearLayout1"
		android:layout_width="fill_parent" android:layout_height="35dp">
		<TextView android:layout_width="wrap_content" android:text="TextView"
			android:id="@+id/textView1" android:layout_height="fill_parent"></TextView>
	</LinearLayout>
	<ListView android:layout_height="wrap_content" android:id="@+id/listView1"
		android:layout_width="fill_parent"></ListView>
</LinearLayout>
